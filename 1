for num in range(1, 101):
    string = ''

    # ここから記述

    #15の倍数の場合
    if num % 15 == 0:
        string = 'FizzBuzz'

    #3の倍数の場合
    elif num % 3 == 0:
        string = 'Fizz'

    #5の倍数の場合
    elif num % 5 == 0:
        string = 'Buzz'

    #その他
    else:
        string = str(num)

    # ここまで記述

    print(string)
